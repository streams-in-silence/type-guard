    "@typescript-eslint/parser": "^5.59.2",
    "@vitest/coverage-c8": "^0.31.0",
    "eslint": "^8.39.0",
    "globals": "^13.20.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4",
    "vite": "^4.3.4",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.31.0"
