{
  "name": "typeguards",
  "version": "0.0.1",
  "description": "A TypeScript utility package to provide simple, type-safe checks.",
  "keywords": [
    "TypeScript",
    "type safe",
    "utility"
  ],
  "bugs": {
    "url": "https://github.com/streams-in-silence/type-guard/issues"
  },
  "license": "Unlicense",
  "author": "Massimo Rosin",
  "files": [
    "dist",
    "UNLICENSE.md",
    "README.md"
  ],
  "main": "dist/type-guard.umd.js",
  "module": "dist/type-guard.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.js"
    }
  },
  "repository": "https://github.com/streams-in-silence/type-guard.git",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/node": "^20.8.3",
    "@types/prettier": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint": "^8.51.0",
    "globals": "^13.23.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vite-plugin-dts": "^3.6.0",
    "vitest": "^0.34.6"
  },
  "packageManager": "yarn@3.4.1"
}
